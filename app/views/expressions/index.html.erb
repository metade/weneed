  </head> 
  <body>
    
    <table height="100%">
      <tr>
	<td>
          <h1>Needs</h1>         
          <div id="exhibit-browse-panel">
            <div ex:role="facet" ex:expression=".needs" ex:facetLabel="Needs" ex:height="20em" ex:width="200px"></div>            
          </div>
       </td>
	<td width="200%" height="100%">
	  <div class="map-lens" ex:role="lens" style="display: none;">
            <div><img ex:src-content=".imageURL" /></div>
            <span ex:content=".details"></span>
          </div>
	  <div ex:role="view"
               ex:viewClass="Map"
               ex:latlng=".latlng"
	       ex:icon=".imageURL"
	       ex:zoom="11"
               ex:shape="square"
	       ex:center='51.4665368, -0.1604018'
	       >
	    
	  </div>
	  </td>
	</tr>
    </table>
  </body>
